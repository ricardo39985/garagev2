<div class="">
  <header class = "nav-wrapper">
    <nav class="green lighten-1">
      <h2><a class="brand-logo black-text center  " href="<%=root_path%>">Car Wishlist</a></h2><br><br>
    </nav> <br>
    <div class = "">
      <div class="col s12 m8">
        <div class= "center-align">
          <% if user_signed_in? %>
                    <%= link_to destroy_user_session_path, method: :delete, class: "green lighten-1 hoverable waves-effect waves-light btn center" do %>
                      <i class="fas fa-sign-out-alt"></i>  Log out
                    <% end %>
                    <%= link_to  new_user_car_path(current_user),class: "green lighten-1 hoverable waves-effect waves-light btn center" do %>
                      <i class="fas fa-plus"></i> Add Car
                    <% end %>
          <% else %>
                      <%= link_to new_user_session_path,class: "green lighten-1 hoverable waves-effect waves-light btn center" do %>
                        <i class="fas fa-sign-in-alt"></i>  Login
                      <% end %>
                      <%= link_to  new_user_registration_path,class: "green lighten-1 hoverable waves-effect waves-light btn center" do %>
                        <i class="fas fa-user-plus"></i>  Sign Up
                    <% end %>
                      <%=link_to user_google_oauth2_omniauth_authorize_path, class: "green lighten-1 hoverable waves-effect waves-light btn center" do %>
                        <i class="fab fa-google"></i>  Sign in with Google
                    <% end %>
          <% end %> 
        </div>
      </div>
    </div>
  </header> 
</div>
  